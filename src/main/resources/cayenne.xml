<?xml version="1.0" encoding="utf-8"?>
<domains project-version="3.0.0.1">
<domain name="KickballData">
	<map name="AmtSolutions" location="AmtSolutions.map.xml"/>
	<map name="KickballDataMap" location="KickballDataMap.map.xml"/>
	<map name="TurkKb" location="TurkKb.map.xml"/>

	<node name="KickballDataNode"
		 datasource="KickballDataNode.driver.xml"
		 factory="org.apache.cayenne.conf.DriverDataSourceFactory">
			<map-ref name="KickballDataMap"/>
	 </node>
	<node name="SolutionData"
		 datasource="SolutionData.driver.xml"
		 factory="org.apache.cayenne.conf.DriverDataSourceFactory">
			<map-ref name="AmtSolutions"/>
	 </node>
	<node name="TurkKb"
		 datasource="TurkKb.driver.xml"
		 factory="org.apache.cayenne.conf.DriverDataSourceFactory"
		 schema-update-strategy="org.apache.cayenne.access.dbsync.CreateIfNoSchemaStrategy">
			<map-ref name="TurkKb"/>
	 </node>
</domain>
</domains>
